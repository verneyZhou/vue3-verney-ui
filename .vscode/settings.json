{
  "editor.formatOnSave": {
    "source.fixAll.eslint": true
  },
  "stylelint.autoFixOnSave": true,  // 保存自动格式化
  "vetur.format.defaultFormatter.html": "js-beautify-html",  // 格式化模板
  "vetur.format.defaultFormatterOptions": {
      "js-beautify-html": {
          "wrap_attributes": "force-aligned",  // 第一个属性后开始折行，并对齐
  },
}